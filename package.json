    "@automerge/automerge-repo-storage-indexeddb": "^2.2.0",
    "@monaco-editor/react": "^4.7.0",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-opener": "^2",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@wasmer/wasi": "^1.2.2",
    "framer-motion": "^12.23.12",
    "katex": "^0.16.22",
    "monaco-editor": "^0.52.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^15.6.1",
    "rehype-katex": "^7.0.1",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.1",
    "remark-html": "^16.0.1",
