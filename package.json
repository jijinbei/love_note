{
  "name": "love_note",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "codegen": "cd src-tauri && cargo run --bin export_schema && cd .. && bunx graphql-codegen --config codegen.ts",
    "codegen:watch": "bun run codegen && bunx graphql-codegen --config codegen.ts --watch",
    "fmt": "bunx prettier --write . && cd src-tauri && cargo fmt",
    "fmt:check": "bunx prettier --check . && cd src-tauri && cargo fmt -- --check"
  },
  "dependencies": {
    "@automerge/automerge": "^3.1.1",
    "@automerge/automerge-repo": "^2.2.0",
